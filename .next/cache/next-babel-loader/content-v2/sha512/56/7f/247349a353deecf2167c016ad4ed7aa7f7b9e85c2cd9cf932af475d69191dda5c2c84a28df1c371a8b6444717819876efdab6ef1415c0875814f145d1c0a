{"ast":null,"code":"import { delay, put } from \"redux-saga/effects\";\nimport { all, fork, takeLatest } from \"@redux-saga/core/effects\";\nimport { ADD_COMMENT_SUCCESS, ADD_COMMENT_REQUEST, ADD_COMMENT_FAILURE, ADD_POST_FAILURE, ADD_POST_SUCCESS, ADD_POST_REQUEST, ADD_POST_TO_ME, REMOVE_POST_OF_ME, REMOVE_POST_FAILURE, REMOVE_POST_REQUEST, REMOVE_POST_SUCCESS } from \"../reducers/post\";\nimport shortId from 'shortid';\n\nfunction* addPost(action) {\n  try {\n    yield delay(1000);\n    /*        const result = yield call(addPostAPI , action.data);*/\n\n    const id = shortId.generate();\n    yield put({\n      type: ADD_POST_SUCCESS,\n      data: {\n        id: id,\n        content: action.data\n      }\n      /*엑션에서 리퀘스트 받아와서 석세스를 넘겨줌 , 데이터에는 사용자가 넣은 데이터가 들어있다.*/\n\n    });\n    yield put({\n      type: ADD_POST_TO_ME,\n      data: id\n    });\n  } catch (err) {\n    yield put({\n      type: ADD_POST_FAILURE,\n      data: err.response.data\n    });\n  }\n}\n\nfunction* removePost(action) {\n  try {\n    yield delay(1000);\n    /*        const result = yield call(addPostAPI , action.data);*/\n\n    yield put({\n      type: REMOVE_POST_SUCCESS,\n      data: action.data\n      /*엑션에서 리퀘스트 받아와서 석세스를 넘겨줌 , 데이터에는 사용자가 넣은 데이터가 들어있다.*/\n\n    });\n    yield put({\n      type: REMOVE_POST_OF_ME,\n      data: action.data\n    });\n  } catch (err) {\n    yield put({\n      type: REMOVE_POST_FAILURE,\n      data: err.response.data\n    });\n  }\n}\n\nfunction* addComment(action) {\n  try {\n    yield delay(1000);\n    yield put({\n      type: ADD_COMMENT_SUCCESS,\n      data: {\n        content: action.data\n      }\n    });\n  } catch (err) {\n    yield delay(1000);\n    yield put({\n      type: ADD_COMMENT_FAILURE,\n      data: action.data\n    });\n  }\n}\n\nfunction* watchAddPost() {\n  yield takeLatest(ADD_POST_REQUEST, addPost);\n  /*요청을 2초에 한번만 받아들임*/\n}\n\nfunction* watchRemovePost() {\n  yield takeLatest(REMOVE_POST_REQUEST, removePost);\n  /*요청을 2초에 한번만 받아들임*/\n}\n\nfunction* watchAddComment() {\n  yield takeLatest(ADD_COMMENT_REQUEST, addComment);\n  /*요청을 2초에 한번만 받아들임*/\n}\n\nexport default function* postSaga() {\n  yield all([fork(watchAddPost), fork(watchAddComment), fork(watchAddPost), fork(watchRemovePost)]);\n}","map":{"version":3,"sources":["I:/react4/src/sagas/post.js"],"names":["delay","put","all","fork","takeLatest","ADD_COMMENT_SUCCESS","ADD_COMMENT_REQUEST","ADD_COMMENT_FAILURE","ADD_POST_FAILURE","ADD_POST_SUCCESS","ADD_POST_REQUEST","ADD_POST_TO_ME","REMOVE_POST_OF_ME","REMOVE_POST_FAILURE","REMOVE_POST_REQUEST","REMOVE_POST_SUCCESS","shortId","addPost","action","id","generate","type","data","content","err","response","removePost","addComment","watchAddPost","watchRemovePost","watchAddComment","postSaga"],"mappings":"AAAA,SAAQA,KAAR,EAAeC,GAAf,QAAyB,oBAAzB;AACA,SAAQC,GAAR,EAAaC,IAAb,EAAmBC,UAAnB,QAAoC,0BAApC;AACA,SACIC,mBADJ,EACyBC,mBADzB,EAC8CC,mBAD9C,EAEIC,gBAFJ,EAEsBC,gBAFtB,EAEwCC,gBAFxC,EAE0DC,cAF1D,EAGIC,iBAHJ,EAGsBC,mBAHtB,EAG0CC,mBAH1C,EAG8DC,mBAH9D,QAIO,kBAJP;AAKA,OAAOC,OAAP,MAAoB,SAApB;;AAGA,UAAUC,OAAV,CAAkBC,MAAlB,EAAyB;AACrB,MAAG;AAEC,UAAMlB,KAAK,CAAC,IAAD,CAAX;AACA;;AACA,UAAMmB,EAAE,GAAGH,OAAO,CAACI,QAAR,EAAX;AACA,UAAMnB,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAAEZ,gBADA;AAENa,MAAAA,IAAI,EAAE;AACFH,QAAAA,EAAE,EAAEA,EADF;AAEFI,QAAAA,OAAO,EAAEL,MAAM,CAACI;AAFd;AAGH;;AALG,KAAD,CAAT;AAOA,UAAMrB,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAACV,cADC;AAENW,MAAAA,IAAI,EAACH;AAFC,KAAD,CAAT;AAIH,GAhBD,CAgBC,OAAMK,GAAN,EAAU;AACP,UAAMvB,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAAEb,gBADA;AAENc,MAAAA,IAAI,EAAEE,GAAG,CAACC,QAAJ,CAAaH;AAFb,KAAD,CAAT;AAIH;AACJ;;AACD,UAAUI,UAAV,CAAqBR,MAArB,EAA4B;AACxB,MAAG;AAEC,UAAMlB,KAAK,CAAC,IAAD,CAAX;AACA;;AACA,UAAMC,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAAEN,mBADA;AAENO,MAAAA,IAAI,EAAEJ,MAAM,CAACI;AAAM;;AAFb,KAAD,CAAT;AAIA,UAAMrB,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAACT,iBADC;AAENU,MAAAA,IAAI,EAACJ,MAAM,CAACI;AAFN,KAAD,CAAT;AAIH,GAZD,CAYC,OAAME,GAAN,EAAU;AACP,UAAMvB,GAAG,CAAC;AACNoB,MAAAA,IAAI,EAAER,mBADA;AAENS,MAAAA,IAAI,EAAEE